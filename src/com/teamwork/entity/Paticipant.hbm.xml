<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.teamwork.entity">
	<class name="Participant">
		<id name="ParticipantId" type="java.lang.Integer">
			<column name="ParticipantId" />
			<generator class="identity" />
		</id>
		<property generated="never" lazy="false" name="UserID"
			type="java.lang.Integer">
			<column name="UserID" />
		</property>
		<property generated="never" lazy="false" name="DateId"
			type="java.lang.Integer">
			<column name="DateId" />
		</property>
		
		
		
	</class>
</hibernate-mapping>